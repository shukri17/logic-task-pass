#Q3
def count_letter(String):
 
 for i in range(0, len(String)):
     count = 1;
     for j in range(i+1, len(String)):
       if(String[i] == String[j] and String[i] != ' '):
          count = count + 1;
          String = String[:j] + '0' + String[j+1:]
     if(count > 1 and String[i] != '0'):
        print(String[i],"=",count)
Str1 = "AI dojo "
Str2="computiq"
Str3="mosul space"
print("str1 :")
count_letter(Str1)
print("str2 :")
count_letter(Str2)
print("str3 :")
count_letter(Str3)








   


          
